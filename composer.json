{
  "name": "ntuangiang/php-ioc",
  "type": "library",
  "description": "This is a design pattern for dependency injection and object locator.",
  "keywords": [
    "ioc",
    "dependency",
    "dependency-injection"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nguyen Tuan Giang",
      "email": "ntuangiang@outlook.com"
    }
  ],
  "support": {
    "issues": "https://github.com/ntuangiang/php-ioc/issues"
  },
  "require": {
    "php": "^7.3",
    "ntuangiang/php-observer": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "dev-master": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "IoC\\": "src/"
    },
    "classmap": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
  }
}

